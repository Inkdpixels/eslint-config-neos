{
  "name": "@neos-project/eslint-config-neos",
  "description": "ESLint shareable config based on XO with a 4-space indent for the neos project.",
  "main": "./index.js",
  "version": "1.1.1",
  "scripts": {
    "test": "npm run ava",
    "test:watch": "npm run ava -- --watch",
    "ava": "ava index.spec.js"
  },
  "ava": {
    "files": [
      "index.spec.js"
    ],
    "failFast": true,
    "verbose": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.13.2",
    "eslint": "^3.1.1",
    "eslint-config-xo-react": "^0.9.0",
    "eslint-config-xo-space": "^0.14.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.0.0",
    "eslint-plugin-react": "^6.0.0",
    "is-plain-obj": "^1.1.0",
    "temp-write": "^2.1.0"
  },
  "peerDependencies": {
    "eslint": "^3.1.1"
  },
  "dependencies": {
    "babel-eslint": "^6.0.4",
    "eslint-config-xo-react": "^0.9.0",
    "eslint-config-xo-space": "^0.14.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^2.0.0",
    "eslint-plugin-react": "^6.0.0",
    "eslint-plugin-import": "^1.13.0"
  },
  "license": "GPL-3.0+",
  "repository": {
    "type": "git",
    "url": "https://github.com/neos/eslint-config-neos.git"
  }
}
